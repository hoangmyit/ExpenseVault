{
  "cSpell.words": [
    "expensevault",
    "languagedetector",
    "luxon",
    "nanoevents",
    "prefersdark",
    "svgr",
    "uuidv",
    "VITE",
     "Ardalis",
    "Mediat"
  ],
  "cSpell.language": "en,vi",
  "cSpell.ignorePaths": [
    "node_modules/**",
    "dist/**",
    "build/**",
    "*.min.*",
    "**/*.d.ts",
    "package-lock.json",
    "yarn.lock",
    "pnpm-lock.yaml"
  ],
  "i18n-ally.localesPaths": ["src/core/i18n"],
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "text": "Always detail the changes made in the commit message. Use a lot of icons to make it more visually appealing."
    }
  ],
  "github.copilot.chat.codeGeneration.instructions": [
    {
      "text": "Always start the response with 'Hi My, '. Always add a comment: 'Generated by Copilot'."
    },
    {
      "file": "dotnet.instructions.md"
    },
    {
      "file": "react.instructions.md"
    }
  ],
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
}
